library(raster)
library(rgbif)
library(tidyverse)

futuro <-
  getData("CMIP5", model = "CC", var = "bio", rcp = 85, res = 10, year = 50)

an_darlingi <-
  occ_search(scientificName = "Anopheles darlingi", limit = 2575)

an_darlingi$data %>%
  select(scientificName, decimalLatitude, decimalLongitude)
